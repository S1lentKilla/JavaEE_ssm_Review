<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="jihe" class="com.haust.pojo.Jihe">
        <property name="lists">
            <list value-type="java.lang.String">
                <value>1</value>
                <value>2</value>
                <value>3</value>
                <value>4</value>
            </list>
        </property>
        <property name="arrs">
            <array value-type="java.lang.Integer">
                <value>5</value>
                <value>6</value>
                <value>7</value>
            </array>
        </property>
        <property name="sets">
            <set value-type="java.lang.String">
                <value>1</value>
                <value>1</value>
                <value>2</value>
            </set>
        </property>
        <property name="maps">
            <map key-type="java.lang.String" value-type="java.lang.String">
                <entry key="1" value="2"></entry>
                <entry key="3" value="5"></entry>
            </map>
        </property>
        <property name="props">
            <props>
                <prop key="1">3</prop>
                <prop key="2">7</prop>
            </props>
        </property>
    </bean>
</beans>